{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.dev.js --inline --progress --port 8080",
    "build": "npm run clean && webpack --config ./config/webpack.dev.js --progress --profile",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "pree2e": "webdriver-manager update",
    "test": "tsc && concurrently \"tsc -w\" \"karma start ./config/karma.conf.js\"",
    "test-once": "tsc && karma start ./config/karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean": "rimraf -- dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "rxjs": "5.0.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/node": "^6.0.46",
    "@types/jasmine": "^2.5.36",
    "jasmine-core": "^2.5.2",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~4.0.14",
    "rimraf": "^2.5.4",
    "typescript": "~2.1.5",
    "tslint": "^4.4.2",
    
    "webpack": "2.2.1",
    "webpack-dev-server": "2.3.0",
    "webpack-merge": "^2.6.1",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.4",
    "css-loader": "^0.26.1",
    "sass-loader": "^6.0.1",
    "node-sass": "^4.5.0",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "raw-loader": "^0.5.1",
    "tslint-loader": "^3.4.2",
    "html-webpack-plugin": "^2.28.0",
    "extract-text-webpack-plugin": "2.0.0-rc.3"
  },
  "repository": {}
}
